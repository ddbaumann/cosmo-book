                RowBox[{
                "coord", "\[LeftDoubleBracket]", "j", 
                 "\[RightDoubleBracket]"}]}], " ", "]"}], "-", 
              RowBox[{"D", "[", 
               RowBox[{
                RowBox[{"metric", "\[LeftDoubleBracket]", 
                 RowBox[{"j", ",", "k"}], "\[RightDoubleBracket]"}], ",", 
                RowBox[{
                "coord", "\[LeftDoubleBracket]", "s", 
                 "\[RightDoubleBracket]"}]}], " ", "]"}]}], ")"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"s", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "n"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "n"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "1", ",", "n"}], "}"}]}], "]"}], " ", "]"}]}]}], 
   ";"}], "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"riemann", ":=", 
    RowBox[{"riemann", "=", 
     RowBox[{"simp", "[", 
      RowBox[{"Table", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"affine", "\[LeftDoubleBracket]", 
            RowBox[{"i", ",", "j", ",", "l"}], "\[RightDoubleBracket]"}], ",", 
