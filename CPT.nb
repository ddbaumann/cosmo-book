Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"inversemetric", "=", 
    RowBox[{"simp", "[", 
     RowBox[{"Inverse", "[", "metric", "]"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
