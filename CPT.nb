         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"affine", "\[LeftDoubleBracket]", 
              RowBox[{"s", ",", "j", ",", "l"}], "\[RightDoubleBracket]"}], 
             " ", 
             RowBox[{"affine", "\[LeftDoubleBracket]", 
              RowBox[{"i", ",", "k", ",", "s"}], "\[RightDoubleBracket]"}]}], 
            "-", 
            RowBox[{
             RowBox[{"affine", "\[LeftDoubleBracket]", 
              RowBox[{"s", ",", "j", ",", "k"}], "\[RightDoubleBracket]"}], 
             " ", 
             RowBox[{"affine", "\[LeftDoubleBracket]", 
              RowBox[{"i", ",", "l", ",", "s"}], 
              "\[RightDoubleBracket]"}]}]}], ",", "\[IndentingNewLine]", 
           RowBox[{"{", 
