         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{"inversemetric", "\[LeftDoubleBracket]", 
              RowBox[{"i", ",", "s"}], "\[RightDoubleBracket]"}], ")"}], "*", 
            "\[IndentingNewLine]", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"D", "[", 
               RowBox[{
                RowBox[{"metric", "\[LeftDoubleBracket]", 
                 RowBox[{"s", ",", "j"}], "\[RightDoubleBracket]"}], ",", 
